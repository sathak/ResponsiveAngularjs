span[mf-dropdown-static-include] {
    /*position: absolute;*/
    width: 94%;
}

    span[mf-dropdown-static-include] table {
        position: absolute;
        width: 95%;
    }

        span[mf-dropdown-static-include] table thead {
            margin-top: -50px;
            /*margin-left: -27px;*/
            position: absolute;
            width: 100%;
            display: inherit;
        }

            span[mf-dropdown-static-include] table thead th {
                border: none;
                padding-top: 0px;
                background-color: #243649;
                color: #E7E7E7;
            }

                span[mf-dropdown-static-include] table thead th:first-child {
                    padding-left: 20px;
                }

                span[mf-dropdown-static-include] table thead th:last-child {
                    padding-left: 18px;
                    /*padding-right: 53%;
                    padding-left: 16%;*/
                }

        span[mf-dropdown-static-include] table tbody {
            margin-top: -20px;
            margin-left: 10px;
            position: absolute;
            width: 97%;
            display: inherit;
        }

            span[mf-dropdown-static-include] table tbody td {
                border: none;
                padding-top: 0px;
                padding-left: 2%;
                white-space: normal;
            }
