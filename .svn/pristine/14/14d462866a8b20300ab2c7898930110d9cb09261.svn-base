<div class="row">
    <div class="col-md-12">
        <navcon-portlet title="Sample Kendo Fields" navcon-data="vm" class="" icon="" type="userManagement" tools-disp="">
            <div class="center" id="User">
                <div class="form-group row margin-bottom-10">
                    <navcon-kendo-table table-settings="vm.userManagementTable" navcon-data="vm" type="userManagement" fields="vm.userManagementFields"></navcon-kendo-table>
                </div>
                <div ng-if="vm.selecteduser">
                    <div class="pull-right statusdiv" style="display: block;" >
                        <ul class="nav navbar-right panel_toolbox" style="margin-top: 0px !important;">
                            <li data-toggle="tooltip" data-original-title="Edit">
                                <a class="collapse-link" ng-click="vm.edit();">
                                    <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                </a>
                            </li>
                            <!--<li data-toggle="tooltip" title="Print" data-original-title="Print">
                                <a class="close-link"><i class="fa fa-print" ng-click="print()"></i></a>
                            </li>-->
                        </ul>
                    </div>

                    <div class="well">
                        <navcon-form navcon-data="vm" footer='' field-data="vm.userManagementFields" type="userManagement" form-manage="vm.formManage" ng-if="vm.userManagementFields!=undefined" read-only="vm.readOnly">
                            <div class="form-group row">
                                <navcon-kendofield label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="BaseId"></navcon-kendofield>
                            </div>
                            <div class="form-group row">
                                <navcon-kendofield label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="DepartmentId"></navcon-kendofield>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="ClockNumber"></navcon-field>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="UserName"></navcon-field>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="FirstName"></navcon-field>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="MiddleName"></navcon-field>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="LastName"></navcon-field>
                            </div>
                            <div class="form-group row">
                                <navcon-kendofield label-class="col-md-3 col-sm-12" field-class="col-md-2 col-sm-6" field-key="DateAndTime"></navcon-kendofield>
                            </div>
                            <div class="form-group row">
                                <navcon-kendofield label-class="col-md-3 col-sm-12" field-class="col-md-2 col-sm-6" field-key="DateAndTimeOfObservation"></navcon-kendofield>
                            </div>
                            <div class="form-group row">
                                <navcon-kendofield label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="ProjectUser"></navcon-kendofield>
                            </div>
                            <div class="form-group row">
                                <navcon-kendofield label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="Roles"></navcon-kendofield>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="EDATrained"></navcon-field>
                            </div>
                            <div class="form-group row">
                                <navcon-field label-class="col-md-3 col-sm-12" field-class="col-md-6 col-sm-12" field-key="UserStatusId"></navcon-field>
                            </div>
                            <!--<div class="form-group row; col-md-12 text-center" ng-hide="vm.isITAR">
                                <navcon-field field-key="ITARWarning" label-class="" field-class="col-md-12 IncreaseWidth"></navcon-field>
                            </div>-->
                        </navcon-form>
                    </div>
                    <div class="form-group row; col-md-12 text-center">
                        <div class="form-group row">
                            &nbsp;
                        </div>
                        <navcon-button class="btn btn-primary center" icon="fa fa-floppy-o" text="Save" ng-click="vm.formManage.save('Save')" ng-hide="vm.isSave"></navcon-button>
                        <navcon-button class="btn btn-primary center" icon="fa fa-floppy-o" ng-if="!vm.readOnly && vm.rights.Edit" text="Update" ng-click="vm.formManage.save('Update')" ng-hide="vm.isUpdate"></navcon-button>
                        <navcon-button class="btn btn-warning center" icon="fa fa-stop" text="Cancel" ng-if="!vm.readOnly" ng-click="vm.cancel()" ng-hide="vm.isCancel"></navcon-button>
                    </div>
                </div>
            </div>
        </navcon-portlet>
    </div>
</div>