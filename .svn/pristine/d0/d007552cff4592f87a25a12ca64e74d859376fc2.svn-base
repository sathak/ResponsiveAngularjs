                <span ng-bind-html="itm[$parent.item.selectOptions.item.displayField || text] | highlight: ((item.selectOptions.customFilter !== undefined && item.selectOptions.customFilter) ? '' : $select.search)" ng-if="!$parent.item.selectOptions.isCustomFormat"></span>
                <span ng-bind-html="$parent.getFormat(itm) | highlight: ((item.selectOptions.customFilter !== undefined && item.selectOptions.customFilter) ? '' : $select.search)" ng-if="$parent.item.selectOptions.isCustomFormat"></span>
