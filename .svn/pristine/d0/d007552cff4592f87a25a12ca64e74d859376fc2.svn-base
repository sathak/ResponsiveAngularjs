        <ol class="navcon-select {{item.templateOptions.customClass}}"
            ng-model="item.data"
            ng-class="item.disabled === true ? 'navcon-disable' : ''"
            ng-disabled="item.disabled">
            <li ng-repeat="itm in item.selectOptions.items" ng-click="tabClick(item,itm)">
                <label class="control-label {{itm.customClass}}" style="width: 100%; text-align: left; padding-left: 8px;" ng-hide="{{item.templateOptions.listtype.toUpperCase() !== 'LABEL'}}">
                    {{itm[$parent.item.selectOptions.item.displayField || text]}}
                </label>
                <textarea class="{{itm.customClass}}" ng-attr-placeholder="{{itm.placeholder}}" rows="{{itm.rows}}" style="width: 100%;"
                          ng-hide="{{item.templateOptions.listtype.toUpperCase() !== 'TEXT'}}">
                    {{itm[$parent.item.selectOptions.item.displayField || text]}}
                </textarea>
            </li>
        </ol>
    </div>
</script>

<script type="html/template" id="tab">
    <div class="navcon-group" ng-hide="item.hidden">
        <label class="navcon-select control-label" ng-hide="item.templateOptions.hideLabel">{{item.templateOptions.label}}</label>
        <ul class="nav nav-pills nav-justified" style="background-color: #eee;"
            ng-model="item.data"
            ng-disabled="item.disabled">
            <li ng-repeat="itm in item.selectOptions.items" ng-click="tabClick(item,itm)" ng-mouseover="tabMouseOver($event, item, itm)"
                ng-class="{'active':item.data == itm[$parent.item.selectOptions.item.dataField || id]}">
                <label class="{{itm.customClass}}" style="width: 98%; color: #000000; background-color: {{itm[$parent.item.selectOptions.item.color]}}"
                       ng-hide="{{item.templateOptions.listtype.toUpperCase() !== 'LABEL'}}">
                    {{itm[$parent.item.selectOptions.item.displayField || text]}}
                </label>
                <a class="{{itm.customClass}}" data-toggle="pill" href='javascript:;' style=" color #000000; background-color {{itm[$parent.item.selectOptions.item.color]}} "
                   data-target="#{{itm[$parent.item.selectOptions.item.dataField || id]}}"
                   ng-hide="{{item.templateOptions.listtype.toUpperCase() !== 'LINK'}}"
                   ng-class="{'navcontabact':item.data == itm[$parent.item.selectOptions.item.dataField || id]}">
                    {{itm[$parent.item.selectOptions.item.displayField || text]}}
                </a>
            </li>
        </ul>
    </div>
</script>

