<script type="html/template" id="input">
    <label class="field-label  {{labelClass}}" ng-class="{ 'has-error': form[item.key].$touched && form[item.key].$invalid}" ng-hide="item.templateOptions.label == ''">{{item.templateOptions.label}}<em ng-show="item.templateOptions.required == 'yes'">*</em></label>
    <label class="field-label-readonly {{fieldClass}}" ng-if="readOnly && (item.templateOptions.hyperlink==undefined|| item.templateOptions.hyperlink=='no')" ng-bind="item.readOnlyText"></label>
    <a class="field-label-readonly {{fieldClass}}" href="" ng-click="redirectURL(item.data)" data-hyperlink="yes" ng-if="readOnly && (item.templateOptions.hyperlink!=undefined && item.templateOptions.hyperlink=='yes')" target="_blank">{{item.data}}</a>

    <div class="field-group {{fieldClass}}" ng-show="!readOnly" data-original-title="{{item.templateOptions.tooltip}}" data-placement="top" data-toggle="tooltip">
        <input name="{{item.key}}" focus-me="item.templateOptions.focus" placeholder="{{item.templateOptions.placeholder}}" class="form-control  {{item.templateOptions.customClass}}" compare-to="item.templateOptions.compareTo" type="text"
               is-exist
               ng-blur="onBlur($event)"
               ng-click="onClick()"
               ng-model="item.data"
               ng-paste="onPaste($event)"
               ng-class="item.disabled === true ? 'navcon-disable' : ''"
               ng-required="item.templateOptions.required == 'yes'"
               ng-minlength="{{item.templateOptions.minlength}}"
               ng-maxlength="{{item.templateOptions.maxlength}}"
               ng-pattern="{{item.templateOptions.pattern}}"
               ng-disabled="item.disabled" spellcheck={{item.templateOptions.spellcheck}}>
        <div class="error-messages error-margin" ng-messages="form[item.key].$error" ng-if="form[item.key].$touched && item.showMessage && !item.setFocus">
            <div ng-style="{width: (item.templateOptions.requiredMessage.length*7)+'px'}" ng-message="required">{{item.templateOptions.requiredMessage || 'Shouldn\'t be empty...'}}</div>
            <div ng-style="{width: (item.templateOptions.minlengthMessage.length*7)+'px'}" ng-message="minlength">{{item.templateOptions.minlengthMessage || 'It\'s too short'}} </div>
            <div ng-style="{width: (item.templateOptions.maxlengthMessage.length*7)+'px'}" ng-message="maxlength">{{item.templateOptions.maxlengthMessage || 'It\'s too long'}}</div>
            <div ng-style="{width: (item.templateOptions.patternMessage.length*7)+'px'}" ng-message="pattern">{{item.templateOptions.patternMessage || 'Invalid value...'}}</div>
            <div ng-style="{width: (item.templateOptions.compareToMessage.length*7)+'px'}" ng-message="compareTo">{{item.templateOptions.compareToMessage || 'Mismatch...'}}</div>
            <div ng-style="{width: (item.templateOptions.compareDateMessage.length*7)+'px'}" ng-message="compareDate">{{item.templateOptions.compareDateMessage || 'Invaid Date...'}}</div>
            <div ng-style="{width: (item.templateOptions.isExistMessage.length*7)+'px'}" ng-message="isExist">{{item.templateOptions.isExistMessage || 'Already available...'}}</div>
        </div>
    </div>
</script>

