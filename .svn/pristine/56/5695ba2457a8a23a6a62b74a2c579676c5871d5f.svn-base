
navcon-dashboard {
    margin: 0;
    width: 100%;
}

.gridster .gridster-item {
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
    background-color: white;
    overflow: hidden;
}

.navcon-widget-header {
    position: relative;
    color: white;
    background-color: #444;
    font-size: 18px;
    border-bottom: 1px solid black;
    min-height: 30px;
    padding: 8px;
    padding-top: 4px;
    padding-bottom: 2px;
}

.navcon-widget-header-buttons {
    position: absolute;
    top: 0;
    right: 0;
}

    .navcon-widget-header-buttons .btn {
        margin: 4px;
        margin-top: 3px;
        padding-top: 1px;
        padding-bottom: 1px;
    }

.navcon-widget-body {
    position: relative;
}

.navcon-dashboard-header {
    font-size: 20px;
    border-bottom: 1px solid #444;
    color: white;
    background-color: #888;
    margin: -20px;
    margin-bottom: 20px;
    min-height: 50px;
    padding: 8px;
    padding-left: 20px;
}

.navcon-dashboard-controls {
    float: right;
}

.navcon-widget-menu-area {
    position: absolute;
    top: 0;
    right: 8px;
    font-size: 22px;
}

    .navcon-widget-menu-area .dropdown-menu {
        left: -20px;
        padding-left: 15px;
    }

    .navcon-widget-menu-area a {
        color: white;
    }

    .navcon-widget-menu-area i {
        cursor: pointer;
    }

.dropdown-backdrop {
    position: static;
}

.navcon-dashboardproject {
    color: #E7E7E7;
    background-color: #243649;
}

