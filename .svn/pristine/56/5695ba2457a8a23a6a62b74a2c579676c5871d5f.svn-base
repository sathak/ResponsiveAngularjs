.widgetSettings {
    position: absolute;
    background: lightgray;
    top: 1px;
    right: 10px;
    margin-top: 25px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    z-index: 1;
}

.gridster-content .portlet .portlet-title {
    color: #E7E7E7;
    background-color: #243649;
}

a.read-more, a.read-less {
    cursor: pointer;
    color: #5a738e;
    text-decoration: none;
    background-color: transparent;
    font-weight: 600;
}

    a.read-more:visited, a.read-more:focus, a.read-less:visited, a.read-less:focus {
        color: #23527c;
        outline: 0;
    }

span.more-text {
    display: none;
}

    span.more-text.show {
        display: inline !important;
    }

.summaryTemplate-image {
    width: 200px;
    background: #fff;
    z-index: 1000;
    position: inherit;
    border: 1px solid rgba(52,73,94,0.44);
    padding: 4px;
    margin-top: -8px;
}
