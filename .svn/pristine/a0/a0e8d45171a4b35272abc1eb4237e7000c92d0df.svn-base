                        <navcon-button class="btn btn-primary center" icon="fa fa-floppy-o" text="check" ng-click="vm.check('')"></navcon-button>
                        <navcon-kendo-tree tree-settings="vm.userAccessRightsTree" navcon-data="vm" type="userAccessRights" fields="vm.userAccessRightsFields"></navcon-kendo-tree>
