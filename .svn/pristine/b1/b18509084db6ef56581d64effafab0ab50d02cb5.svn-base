<style>
    .pageContainer {
        height: 100%;
        padding-top: 15%;
    }

    html, body {
        height: 100% !important;
    }

    .page-content {
        padding: 0;
        height: auto !important;
    }

    #pagecontent {
        margin: 0;
    }

    #loginDiv {
        /* IE10+ */
        background-image: -ms-linear-gradient(bottom right, #D0D3D4 0%, #A2AAAD 38%, #D0D3D4 70%, #A2AAAD 100%);
        /* Mozilla Firefox */
        background-image: -moz-linear-gradient(bottom right, #D0D3D4 0%, #A2AAAD 38%, #D0D3D4 70%, #A2AAAD 100%);
        /* Opera */
        background-image: -o-linear-gradient(bottom right, #D0D3D4 0%, #A2AAAD 38%, #D0D3D4 70%, #A2AAAD 100%);
        /* Webkit (Safari/Chrome 10) */
        background-image: -webkit-gradient(linear, right bottom, left top, color-stop(0, #D0D3D4), color-stop(38, #A2AAAD), color-stop(70, #D0D3D4), color-stop(100, #A2AAAD));
        /* Webkit (Chrome 11+) */
        background-image: -webkit-linear-gradient(bottom right, #D0D3D4 0%, #A2AAAD 38%, #D0D3D4 70%, #A2AAAD 100%);
        /* W3C Markup */
        background-image: linear-gradient(to top left, #D0D3D4 16%, #A2AAAD 38%, #D0D3D4 65%, #A2AAAD 100%);
    }

    i {
        color: #a8a8a8;
    }

    .fpwd > a {
        color: #fff;
    }

    .field-group > input {
        height: 50px;
        font-size: 20px;
        font-family: 'loginBold';
    }

    .btncolor {
        padding: 8px;
        font-size: 20px;
        width: 150px;
        float: right;
        border-radius: 5px !important;
    }

    .fontLetter {
        font-size: 20px;
    }

    .ssoerror {
        text-align: center;
        font-size: 24px;
        color: red;
    }

    .ssoinprogress {
        text-align: center;
        font-size: 24px;
        color: green;
    }

    #footer {
        position: absolute;
        right: 0;
        bottom: 0;
        left: 0;
        padding: 1rem;
        text-align: center;
    }
</style>
<div id="loginDiv" style="height:100%">
    <div class="page-logo col-md-2" style="padding:40px;">
        <a href="index.html">
            <img src="Content/layout/img/BAElogo.png" alt="logo" class="logo-default" style="width:350px">
        </a>
        <div class="menu-toggler sidebar-toggler hide">
            <!-- DOC: Remove the above "hide" to enable the sidebar toggler button on header -->
        </div>
    </div>
    <!-- Begin page content -->
    <div class="pageContainer">
        <navcon-form navcon-data="vm" footer='' field-data="vm.loginFields" type="loginPage">
            <h3 style="font-family: 'loginBold'; text-transform: uppercase; letter-spacing: 25px;text-align:center; font-size: 60px; color: #F56323;">RMS</h3>
            <div id="login-wraper" class="loginbox" ng-if="!vm.SSO">
                <div>
                    <div style="font-family:'loginBold' ;vertical-align:middle; font-size:25px;padding:10px;color:#fff;background:#d02239;text-align:center;background:#4c4c4c;border-radius: 5px 5px 0px 0px !important;">
                        LOGIN
                    </div>
                </div>
                <div class="ui-formlogin">
                    <!-- Heading -->
                    <!-- Form -->
                    <div style="background:#5F6364;padding:10%;padding-left:5%;padding-right:5%">
                        <div class="form-group row">
                            <!--<label class="loginicon" style="z-index:99"><i class="fa fa-user" style="z-index:99"></i></label>-->
                            <div class="inner-addon left-addon">
                                <i class="glyphicon fa fa-user" style="z-index:99; font-size: 20px;"></i>
                                <navcon-field class="col-md-12" field-key="username" login-enter="vm.enterKey()" style="float:left;"></navcon-field>
                            </div>


                        </div>
                        <div class="form-group row">

                            <div class="inner-addon left-addon">
                                <i class="glyphicon fa fa-lock" style="z-index:99; font-size: 20px;"></i>
                                <navcon-field class="col-md-12 inner-addon left-addon" field-key="userpassword" login-enter="vm.enterKey()"></navcon-field>
                            </div>

                        </div>
                    </div>
                    <div class="form-group row" style="clear:both;padding-top:20px;padding-right:5px;padding-bottom:12px;">

                        <div style="width:50%;float:right;">
                            <navcon-button icon="glyphicon" text="Login" class="col-md-12 warningLogin" ng-click="vm.warningModal('warning')" custom-class="btn login-button btn-group-lg btn-block btncolor"></navcon-button>
                        </div>
                        <div style="width:50%;float:left;">
                            <!--<navcon-button icon="glyphicon" text="Forgot password?" data-target='forgotPassword' data-toggle='modal' class="col-md-12 fpwd" custom-class="btn login-button btn-group-lg btn-block fontLetter"></navcon-button>-->
                        </div>
                    </div>
                </div>


            </div>
            <div class="ssoinprogress" ng-if="vm.SSOProgress">Login in progress.....</div>
            <div class="ssoerror">{{vm.ssoError}}</div>
        </navcon-form>
        <div id="footer">
            <div class="container text-center">
                <p>
                    &copy; <a href="#">NAVCON Aerospace. All rights reserved. </a>
                    <a href="">Privacy policy</a>
                </p>
            </div>
        </div>
        <navcon-modal navcon-data="this" title="ITAR Warning" icon="" id="warning" footer="" size="navcon-modal-md">
            <div style="width:100%;height:100%;margin: 0px; padding:0px" class="col-md-12">
                <div style="font-size:16px;" class="col-md-8">
                    <!--<h4 style="font-weight: bold">ITAR Warning!</h4>-->
                    <p>I confirm that any information I upload does not violate the security classifications approved for this site and network.</p>
                    <p>I further confirm that the content is not technical data, as defined by <b> International Traffic in Arms Regulations (ITAR)</b> or technology, as defined by <b> Export Administration Regulations (EAR)</b>.</p>
                </div>
                <div style="width: 25%;margin: 56px -15px 0px;" class="col-md-4">
                    <i class="fa fa-warning" style="font-size: 140px;color:#da4f49"></i>
                </div>
                <!--<div style="width: 25%; margin: 56px 0px 0px; float: right; padding: 10px;" class="col-lg-8">
                    <i class="fa fa-warning" style="font-size: 140px;color:#da4f49"></i>
                </div>
                <div style="width:70%;margin: 0px; padding:0px;float:left;padding-top: 10px;padding-left: 3%;font-size: 16px;text-align: justify;" class="col-lg-4">
                    <h4 style="font-weight: bold">ITAR Warning!</h4>
                    <p>I confirm that any information I upload does not violate the security classifications approved for this site and network.</p>
                    <p>I further confirm that the content is not technical data, as defined by <b> International Traffic in Arms Regulations (ITAR)</b> or technology, as defined by <b> Export Administration Regulations (EAR)</b>.</p>

                </div>-->
            </div>
            <div class="form-group row">&nbsp;</div>
            <div class="col-md-12 text-center" style="margin-top: 10px;text-align: center;">
                <navcon-button icon="fa fa-unlock" aria-hidden="true" class="btn btn-success" text="I Agree, Login" ng-click="vm.login()" style="width: 200px;border:none !important"></navcon-button>
                <navcon-button icon="fa fa-lock" aria-hidden="true" class="btn btn-danger" text="Logout" ng-click="vm.logout()" style="width: 200px;margin-left:0px !important;" data-dismiss="modal"></navcon-button>

                <!--<a href="#" icon="fa fa-sign-in" class="btn btn-success" aria-hidden="true" ng-click="vm.login()" style="width: 200px">I Agree, Login</a>-->
                <!--<a href="#" class="btn btn-danger" aria-hidden="true" ng-click="vm.logout()" data-dismiss="modal" style="width: 200px">Logout</a>-->
            </div>
        </navcon-modal>
    </div>

</div>







