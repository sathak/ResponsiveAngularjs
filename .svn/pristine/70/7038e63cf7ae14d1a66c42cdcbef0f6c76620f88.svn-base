<script id="print-template" type="x/kendo-template">
    <div class="k-page-template">
        <div class="header">            
            #=title#
        </div>
        <div class="watermark">Navcon Aerospace</div>
        <div class="footer">
            <div style="float: left">Date #=date#</div>
            <div style="float: right">
                Page #: pageNum # of #: totalPages #
            </div>
        </div>
    </div>
</script>

<script id="toolbar-template" type="text/x-kendo-template">
    <a class="k-button" href="\#" onclick="return toolbar_click()">Command</a>
</script>
<script>
    function toolbar_click() {
        console.log("Toolbar command is clicked!");
        return false;
    }
</script>