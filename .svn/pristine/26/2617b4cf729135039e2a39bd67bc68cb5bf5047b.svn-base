<!--<navcon-pagebar breadcrumbs="vm.breadcrumbs"></navcon-pagebar>-->

<div class="row">
    <div class="col-md-12">
        <navcon-portlet title="Bulletin Management" navcon-data="vm" class="" icon="" type="NewsAnnouncements" tools-disp="">
            <navcon-table table-settings="vm.newsTable" navcon-data="vm" type="NewsAnnouncements" css-class="rowPointer"></navcon-table>
            <navcon-button class="btn btn-primary center" icon="fa fa-plus" text="Add New" ng-click="vm.addNew('NewsAnnouncements')"></navcon-button>

            <div class="pull-right statusdiv" style="display: block;" ng-if="vm.isEdit">
                <ul class="nav navbar-right panel_toolbox" style="margin-top: 0px !important;">
                    <li data-toggle="tooltip" data-original-title="Edit" ng-if="vm.isEdit" ng-hide="!vm.isSaveHide">
                        <a class="collapse-link" ng-click="vm.edit('NewsAnnouncements');">
                            <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                        </a>
                    </li>
                    <!--<li data-toggle="tooltip" title="Print" data-original-title="Print">
                        <a class="close-link"><i class="fa fa-print" ng-click="print()"></i></a>
                    </li>-->
                </ul>
            </div>

            <div class="collapse center" id="NewsAnnouncements">
                <div class="form-group row">
                    &nbsp;
                </div>
                <div class="well">
                    <navcon-form navcon-data="vm" footer='' field-data="vm.newsFields" type="NewsAnnouncements" form-manage="vm.formManage" ng-if="vm.newsFields!=undefined" read-only="vm.readOnly">
                        <div class="form-group row">
                            <navcon-field label-class="col-md-3" field-class="col-md-6" field-key="NewsTitle"></navcon-field>
                        </div>
                        <div class="form-group row">
                            <navcon-field label-class="col-md-3" field-class="col-md-4" field-key="EffectiveDate"></navcon-field>
                            <navcon-field label-class="col-md-3" field-class="col-md-3" field-key="Important"></navcon-field>
                        </div>
                        <div class="form-group row">
                            <navcon-field label-class="col-md-3" field-class="col-md-6" field-key="ExpiryDate"></navcon-field>
                        </div>
                        <div class="form-group row">
                            <navcon-field field-key="fuattachments" class="col-md-offset-2 col-md-8"></navcon-field>
                        </div>
                        <div class="form-group row">
                            <navcon-field label-class="col-md-3" field-class="col-md-6" field-key="IsPublish"></navcon-field>
                        </div>
                        <div class="form-group row">
                            <navcon-field label-class="col-md-3" field-class="col-md-6" field-key="Summary"></navcon-field>
                        </div>
                        <div class="form-group row; col-md-12 text-center" ng-if="!vm.isSaveHide">
                            <navcon-field field-key="ITARWarning" label-class="" field-class="col-md-12 IncreaseWidth"></navcon-field>
                        </div>
                    </navcon-form>
                </div>
                <div class="form-group row; col-md-12 text-center">
                    <div class="form-group row">
                        &nbsp;
                    </div>
                    <navcon-button class="btn btn-primary center" icon="fa fa-floppy-o" text="Save" ng-click="vm.formManage.save('NewsAnnouncements','Save')" ng-hide="vm.isSaveHide" ng-disabled="!vm.itarwarning"></navcon-button>
                    <navcon-button class="btn btn-warning center" icon="fa fa-stop" text="Close" ng-click="vm.cancel('NewsAnnouncements')" ng-if="vm.newsFields.mode.toLowerCase() ==='save'" ng-hide="vm.isSaveHide"></navcon-button>

                    <navcon-button class="btn btn-warning center" icon="fa fa-stop" text="Cancel" ng-click="vm.cancel('NewsAnnouncements')" ng-if="vm.newsFields.mode.toLowerCase() ==='update'" ng-hide="vm.isSaveHide"></navcon-button>

                    <div class="col-md-4 text-right">
                        <navcon-button class="btn btn-danger center" icon="fa fa-trash" text="Delete" ng-click="vm.delete('NewsAnnouncements')" ng-if="vm.newsFields.mode.toLowerCase() ==='update'" ng-hide="vm.isSaveHide"></navcon-button>
                    </div>
                </div>                
            </div>
        </navcon-portlet>
    </div>
</div>

<navcon-delete navcon-data="vm" title="Deletion Warning !" class="" icon="fa fa-cogs" footer='' type="NewsAnnouncements" size="navcon-modal-sm">
    <div>
        <h2 style="text-align:center">Are you sure you want to delete this Bulletin Management?</h2>
    </div>
    <hr />
    <button style="float:right;" type="button" class="btn btn-danger" ng-click="vm.tableRowDelete('NewsAnnouncements','No')">No</button>
    <button style="float:right;" type="button" class="btn btn-success" ng-click="vm.tableRowDelete('NewsAnnouncements','Yes')">Yes</button>
</navcon-delete>

