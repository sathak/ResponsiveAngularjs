<navcon-portlet title="{{title}}" navcon-data="this" class="" icon="" type="mySpaceChart_{{widgetId}}" tools-disp="" show-caption="true">
    <div class="row" ng-hide="reportList.length <= 1 || !options.filter">
        <div class="col-md-push-6 col-md-6" style="padding-right: 25px;">
            <div id="reportrange_{{widgetId}}" class="pull-right" style="background: #fff; cursor: pointer; padding: 4px 8px; border: 1px solid #ccc">
                <i class="glyphicon glyphicon-calendar fa fa-calendar navcon-dashboard-icon" style="color: green;"></i>
                <span ng-model="chartFilterData"></span>&nbsp;<b class="caret"></b>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <navcon-fchart data="data" events="events"></navcon-fchart>
        </div>
    </div>
</navcon-portlet>