body {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif !important;
    font-size: 13px;
}

.k-item {
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif !important;
    font-size: 14px !important;
}

.k-page-template {
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

    .k-page-template .header {
        position: absolute;
        top: 30px;
        left: 30px;
        right: 30px;
        border-bottom: 1px solid #888;
        color: #888;
    }

    .k-page-template .footer {
        position: absolute;
        bottom: 30px;
        left: 30px;
        right: 30px;
        border-top: 1px solid #888;
        text-align: center;
        color: #888;
    }

    .k-page-template .watermark {
        font-weight: bold;
        font-size: 400%;
        text-align: center;
        margin-top: 30%;
        color: #aaaaaa;
        opacity: 0.1;
        transform: rotate(-35deg) scale(1.7, 1.5);
    }

.k-draghandle.k-state-selected:hover, .k-ghost-splitbar-horizontal, .k-ghost-splitbar-vertical, .k-list > .k-state-highlight,
.k-list > .k-state-selected, .k-marquee-color, .k-panel > .k-state-selected, .k-scheduler .k-scheduler-toolbar .k-state-selected,
.k-scheduler .k-today.k-state-selected, .k-state-selected, .k-state-selected:link, .k-state-selected:visited, .k-tool.k-state-selected {
    color: #000000;
    background-color: blanchedalmond;
    border-color: lightgray;
}

    .k-panelbar > li.k-state-default > .k-link.k-state-selected, .k-panelbar > li > .k-state-selected, .k-state-selected > .k-link {
        color: #000;
    }

/*Grid*/
.k-grid-norecords-template {
    width: 99.9%;
}

.k-grid-content {
    max-height: 600px;
}

.k-grid td.k-state-selected:hover, .k-grid tr.k-state-selected:hover {
    background-color: #bdb4af;
    background-image: none;
}

.k-filtercell .k-dropdown-operator {
    /*width: 35px !important;*/
    right: 0em;
}

.k-filtercell .k-header .k-input {
    padding: 5px;
    border-radius: 4px !important;
}

.k-pager-sizes .k-dropdown {
    width: 60px !important;
}
/*Grid*/

/*Scheduler*/
.k-event, .k-task-complete {
    border-color: #ffc09c;
    background: #aaaaaa 0 -257px url(textures/highlight.png) repeat-x;
    color: #2e2e2e;
}

.k-scheduler-agendaweek {
    position: inherit;
    color: black;
}

.k-scheduler-content .k-event {
    background-color: transparent;
    border-color: transparent;
}

.k-event {
    border-radius: 125px !important;
    -webkit-border-radius: 125px !important;
    -moz-border-radius: 125px !important;
    padding: 4px 0px 6px 0px;
    margin-top: 5px;
    -webkit-border-radius: 10px !important;
    -moz-border-radius: 10px !important;
    border-radius: 10px !important;
}

.gridcell .k-scheduler-event-title {
    padding-left: 10px;
    color: #ffffff;
}

.k-task .k-scheduler-event-title {
    padding-left: 10px;
    color: #000000;
}

/*.k-scheduler-table > tbody > tr > td, .k-scheduler-table > tbody > tr > th {
    border: none;
}*/

.kendoLegend {
    margin-top: 5px;
    margin-bottom: 15px;
    margin-left: -10px;
    border: none;
}

    .kendoLegend .legend {
        padding: 10px;
    }

        .kendoLegend .legend .mark {
            display: inline-block;
            width: 1em;
            height: 1em;
            vertical-align: middle;
            margin-bottom: 5px;
            margin-right: .5em;
            -webkit-border-radius: 5px !important;
            -moz-border-radius: 5px !important;
            border-radius: 5px !important;
        }

        .kendoLegend .legend .title {
            vertical-align: middle
            /*padding: 10px;
            padding-left: 10px;
            color: #000000;            
            padding-bottom: 5px;*/
        }

/*Scheduler*/

.k-chart-crosshair-tooltip, .k-chart-shared-tooltip, .k-widget.k-tooltip {
    border-color: rgba(100,100,100,.9);
    background-color: rgba(100,100,100,.9);
    color: #fff;
}

.k-callout-s {
    border-top-color: rgba(100,100,100,.9);
}

/*field Controls*/
k-datetimepicker .k-picker-wrap {
    padding-right: 0px !important;
    width: 100% !important;
}

.field-group .k-dropdown-wrap .k-input {
    padding: 5px;
    border-radius: 4px !important;
}

.k-multiselect-wrap {
    min-height: 34px;
}


    .k-multiselect-wrap k-floatwrap {
        min-height: 34px;
    }

    .k-multiselect-wrap k-state-hover {
        background: none !important;
    }

.k-calendar td.k-state-selected .k-link, .k-calendar td.k-today.k-state-selected.k-state-hover .k-link {
    color: #000;
    font-weight: bold;
    background: #FFDB99;
}

.k-multiselect.k-header.k-state-focused {
    background: #FFF !important;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    /*border:none !important;*/
    box-shadow: none !important;
    -webkit-box-shadow: none !important;
}

.k-widget .field-date-input {
    width: 95% !important;
}

.k-autocomplete .k-input, .k-multiselect-wrap, .k-textbox > input {
    height: auto !important;
}

.k-autocomplete, .k-block, .k-calendar .k-link, .k-calendar td, .k-calendar-container, .k-colorpicker, .k-combobox, .k-datepicker, .k-datetimepicker, .k-drag-clue, .k-dropdown, .k-dropdown-wrap, .k-editor-inline, .k-gantt-views > .k-current-view, .k-grid .k-filter-options, .k-grouping-header .k-group-indicator, .k-inline-block, .k-list-container, .k-multiselect, .k-notification, .k-numeric-wrap, .k-numerictextbox, .k-picker-wrap, .k-slider-selection, .k-slider-track, .k-split-button .k-gantt-views, .k-textbox, .k-tile, .k-timepicker, .k-tooltip, .k-touch-scrollbar, .k-treeview .k-in, .k-upload, .k-window, .k-window-action, .k-window-titleless .k-window-content {
    height: auto !important;
}

.k-list-container {
    background: #fff;
}

.k-list {
    height: auto;
    max-height: 200px;
    overflow: auto;
}

.k-state-focused, .k-list-container {
    background: #fff;
    border: 1px solid #66afe9 !important;
    outline: 0 !important;
}

.k-dropdown .k-state-focused, .k-filebrowser-dropzone, .k-list > .k-state-hover, .k-mobile-list .k-edit-label.k-check:active, .k-mobile-list .k-item > .k-label:active, .k-mobile-list .k-item > .k-link:active, .k-mobile-list .k-recur-view .k-check:active, .k-pager-wrap .k-link:hover, .k-scheduler .k-scheduler-toolbar ul li.k-state-hover, .k-splitbar-horizontal-hover:hover, .k-splitbar-vertical-hover:hover, .k-state-hover, .k-state-hover:hover {
    background: #F5F5F5 !important;
}

.k-list > .k-state-selected {
    background: #3875d7 !important;
    color: #fff !important;
    border: none !important;
    box-shadow: none !important;
    -webkit-box-shadow: none !important;
}

.k-list .k-state-focused {
    border: none !important;
    box-shadow: none !important;
    -webkit-box-shadow: none !important;
}

/*field Controls*/
