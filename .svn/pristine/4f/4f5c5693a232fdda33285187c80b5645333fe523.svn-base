<div class="modal container fade" tabindex="-1" style="display: none;" data-backdrop="static" data-keyboard="false">
    <div class="content" style="cursor: default">
        <div class="modal-header" style="cursor: move">
            <button class="close" type="button" ng-click="cancel(type)"></button>
            <h4 class="modal-title {{icon}}"> <span style="font-family: 'Open Sans', sans-serif; font-size: 16px;">{{title}}</span></h4>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12" ng-transclude="">
                </div>
            </div>
        </div>
        <div class="modal-footer" fields="fields" style="cursor: move">
            <button ng-show="footer.indexOf('S') !== -1" type="button" ng-click="save(type)" class="btn default box">{{navconData.mode || 'Save'}}</button>
            <button ng-show="footer.indexOf('O') !== -1" type="button" ng-click="navconData.ok(type)" class="btn default box">Ok</button>
            <button ng-show="footer.indexOf('C') !== -1" type="button" ng-click="navconData.cancel(type)" class="btn default box">Cancel</button>
        </div>
    </div>
</div>

